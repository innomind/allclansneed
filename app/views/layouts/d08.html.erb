<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN"
    "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">

<html xmlns="http://www.w3.org/1999/xhtml">

<head>
<title></title>

<%# bundle do %>
  <%= javascript_include_tag :defaults%>
  <%= stylesheet_link_tag "calendar/grey/style.css" %>
  <%= stylesheet_link_tag "d08/layout" %>
  <%= stylesheet_link_tag "d08/style" %>
  <%# javascript_include_tiny_mce_if_used %>
  <%# tiny_mce if using_tiny_mce? %>
  <%= yield :header_tags %>
<%# end %>

</head>

<body>


<div class="wrapper">
    <div class="top">
        <div class="top_left">hallo</div>
        <div class="top_right">hallo</div>
    </div>
    <div class="logo_box">

	<% get_boxes_for("topleft").each do |b| %>
			  <%#= b.name %>
              <%= create_box(b,["<p class='logo_box_button'><span class='logo_box_button_text'>","</span></p>"]) %>
    <% end %>
     
    <p class="logo_box_end"></p>
    </div>
    <div class="logo">

        <div class="logo_subnavi">
			History: <%= show_breadcrumb %>
		    <%# get_boxes_for("topnav").each do |b| %>
					  <%#= b.name %>
	                  <%#= create_box(b,["<span>","</span>"]) %>
	        <%# end %>
		</div>
		
		<div class="logo_text">Dein Clan</div>
    </div>
        <div class="box">
	
		<% get_boxes_for("leftside").each do |b| %>
			<p class="box_cat"><span><%= b.name %></span></p>
			<p class="box_start"></p>
		    <%= create_box(b,["<p class='box_list'><span>","</span></p>"]) %>
			<p class="box_end"></p>
		<% end %>

        </div>
        <div class="content">
			<%if flash[:notice]%><p style="color: green"><%= flash[:notice] %></p>
			<%end%>
			<%if flash[:error]%><p style="color: red"><%= flash[:error] %></p>
			<%end%>
            <p class="content_cat"><span>Content</span></p>
            <div class="content_box_start"></div>
            <div class="content_box"> 
				<%= yield %>
			</div>
            <div class="content_box_end"></div>
        </div>
        <div class="box_right">
		
		<% get_boxes_for("rightside").each do |b| %>
			<p class="box_cat"><span><%= b.name %></span></p>
			<p class="box_start"></p>
		    <%= create_box(b,["<p class='box_list'><span>","</span></p>"]) %>
			<p class="box_end"></p>
		<% end %>
			
        </div>
    <div class="footer">
        <div class="footer_left">gg</div>
        <div class="footer_right">gg</div>
    </div>
</div>

</body>

</html>