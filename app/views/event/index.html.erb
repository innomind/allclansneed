<pre>
<%= calendar({:year => @year, :month => @month, :first_day_of_week => 1, :abbrev => (0..-1), :previous_month_text   => link_to("<<", :year => @year, :month => @month-1), :next_month_text   => link_to(">>", :year => @year, :month => @month+1)}) do |d|
      cell_text = link_to(d.mday, :action => "showDay", :date => d) + "<br />"
      cell_attrs = {:class => 'day'}
        @events.each do |e|
          if e.expire_at.to_date == d
            cell_text << link_to(e.name, :action => "showEvent", :id => e.id)  << "<br />"
            cell_attrs[:class] = 'specialDay'
          end
        end
      [cell_text, cell_attrs]
    end %>
</pre>