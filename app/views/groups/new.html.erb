<% error_handling_form_for :group, @group, :url => {:action => "create"}, :html => { :multipart => false } do |form| %>
      <%= form.text_field :name %>
      <%= form.check_box(:moderated, {}, "true", "false") %>
      <%= form.text_area :description %>
      <%= form.submit "erstellen" %>
<% end %>