<% error_handling_form_for @squad, :url => confirm_users_save_squad_path(@squad) do |f| %>
	<% @squad.users.each do |u| %>
		<%= u.nick %>
		<%= select("squad_user", u.id, [["aus dem Clan löschen","delete"]] + @clan.squads.collect{|s| ["verschieben nach: "+s.name, s.id]})%><br />
		<%= f.submit "squad löschen" %>
	<% end %>
<% end %>