<%#= javascript_include_tag :defaults %>
<h1> alle squads von <%=@clan.name%></h1>
<%@clan.squads.each do |sq|%>
  <div id=<%=  dom_id(sq) %>></div>
  <%  javascript_tag({}) do %>
    <%=remote_function  :url => {:controller => 'squad_user', :action => 'index', :squad_id => sq.id}, :update => dom_id(sq), :method => :get %>
  <%end%>
<%end%>
